TCPKeepAlive yes
ServerAliveInterval 30
Compression yes

Host *
  ForwardAgent yes
  HashKnownHosts no
  GSSAPIAuthentication no
  SendEnv AWS_*

Host github.com
  Hostname ssh.github.com
  User git
  Port 443

Host *.u
  ProxyCommand socat SOCKS4a:127.0.0.1:$(echo -n %h | sed s/.u$//):%p,socksport=1080 STDIO
  User milhouse

Host vengerberg.rep
  User renan
  Port 2272

