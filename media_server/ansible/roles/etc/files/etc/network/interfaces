source /etc/network/interfaces.d/*

auto lo
iface lo inet loopback

allow-hotplug eth0
iface eth0 inet dhcp

auto wlan0
iface wlan0 inet dhcp
            wpa-ssid repdoxinho
            wpa-pairwise CCMP
            wpa-group CCMP
            wpa-psk 963eabfd9fe40f06c1dfced491a041354e127fdf6f2810625857296368cce6a3
